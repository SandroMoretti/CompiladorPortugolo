algoritmo teste2portugolo
   declare numerico
   x, y;
   logico z;
   
   escreva("informe um numero: ");
   leia(x);

   escreva("informe outro numero: ");
   leia(y);

   // primeira atribuicao
   z <-- (x + y) <> (x * y / 2.11);
   escreva(z);

   z <-- (verdadeiro Ou falso) = (falso E falso); // segunda atribuicao
   escreva(z);

   // terceira atribuicao
   se(x > 1 ou (2+2 = 4)) inicio
      escreva("Eh isso ai.");
   fim
fim algoritmo 
